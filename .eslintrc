{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "no-unused-vars": ["warn", { "args": "none" }],
    "max-len": "off",
    "no-shadow": "off",
    "no-plusplus": "off",
    "no-param-reassign": ["error", { "props": false }],
    "linebreak-style": "off"
  }
}
